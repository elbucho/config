{
  "name": "elbucho/config",
  "description": "Multiple file format object-oriented config system",
  "keywords": ["config","ini","xml","json","yaml","yml"],
  "type": "library",
  "minimum-stability": "dev",
  "license": "MIT",
  "authors": [
    {
      "name": "Michael Hinkson",
      "email": "mjhinkson@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "doctrine/inflector": "^1.0"
  },
  "require-dev": {
    "php": ">=7.1",
    "phpunit/phpunit": "^7.2",
    "phpunit/php-code-coverage": "^6.0",
    "symfony/yaml": "*",
    "ext-json": "*",
    "ext-simplexml": "*"
  },
  "autoload": {
    "psr-4": {
      "Elbucho\\Config\\": "src/elbucho/config/"
    }
  },
  "autoload-dev": {
    "Elbucho\\Config\\Tests\\": "tests/elbucho/config/tests/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  },
  "suggest": {
    "ext-simplexml": "Needed to support the XML config file format",
    "ext-json": "Needed to support the JSON config file format",
    "symfony/yaml": "Needed to support the YAML/YML config file formats"
  }
}